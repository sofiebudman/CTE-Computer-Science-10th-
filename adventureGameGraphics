import turtle

turtle.forward(10)
turtle.left(90)
turtle.forward(130)
turtle.left(90)
turtle.forward(10)
turtle.left(90)
turtle.forward(130)
turtle.penup()
turtle.left(90)
turtle.forward(10)
turtle.left(90)
turtle.forward(85)
turtle.pendown()
turtle.right(90)
turtle.forward(60)
turtle.left(90)
turtle.forward(45)
turtle.left(90)
turtle.forward(60)
turtle.penup()
turtle.left(90)
turtle.forward(10)
turtle.left(90)
turtle.forward(20)
turtle.pendown()
turtle.write('^ %$d*@!~')
turtle.penup()
